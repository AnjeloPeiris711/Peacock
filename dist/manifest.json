{
  "manifest_version": 3,
  "name": "Peacock",
  "version": "0.1.0",
  "icons": {
      "16": "/static/media/peacock16.png",
      "48": "/static/media/peacock48.png",
      "128": "/static/media/peacock128.png"
  },
  "action": {
    "default_icon": "/static/media/peacock16.png"
  },
  "background": {
    "service_worker": "/static/js/background.js"
  },
  "options_page":"/static/option/option.html",
  "permissions": [
    "tabs",
    "nativeMessaging",
    "storage"
  ]
}